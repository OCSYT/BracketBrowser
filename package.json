{
  "name": "BracketBrowser",
  "version": "1.0.0",
  "description": "A web browser made with electron",
  "main": "src/index.js",
  "scripts": {
    "start": "electron .",
    "build": "npx electron-packager . BracketBrowser --protocol=http --protocol-name='http' --protocol=https --protocol-name='https' --protocol=file --protocol-name='file' --platform=linux,win32 --arch=all --icon='src/renderer/img/BracketBrowser.ico' --overwrite --out='dist/'"
  },
  "repository": "https://github.com/OCSYT/BracketBrowser",
  "keywords": [
    "Electron",
    "Browser"
  ],
  "author": "OCSYT & oxmc",
  "license": "LGPL-3.0-or-later",
  "devDependencies": {
    "@electron-forge/cli": "^7.1.0",
    "electron": "^27.0.4"
  },
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.26.12",
    "closest-match": "^1.3.3",
    "electron-context-menu": "^3.6.1",
    "electron-tabs": "^1.0.2",
    "electron-window-state": "^5.0.3",
    "is-valid-domain": "^0.1.6"
  },
  "protocol_handlers": [
    {
      "protocol": "http",
      "name": "BracketBrowser"
    },
    {
      "protocol": "https",
      "name": "BracketBrowser"
    },
    {
      "protocol": "file",
      "name": "BracketBrowser"
    }
  ]
}